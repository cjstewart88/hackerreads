<section id="reads">
  <ol id="reads-list" class="loading">
    <script id="read" type="text/html">
      <li class="read">
        <a href="{{url}}" target="_blank" class="read-link">
          {{title}}
        </a>   
             
        <div class="meta">
          <span class="time-ago">{{timeAgo}}</span> - 
          <a href="{{commentUrl}}" target="_blank" class="comment-link">{{commentCount}} comments</a>
        </div>   
      </li>
    </script>
  </ol>
  
  <script>
    $("#reads-list").readRetriever();
  </script>
</section>